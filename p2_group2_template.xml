<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="./schema_1111.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<DOC>

    <article>

        <header>
            <article_title></article_title>
            <author xml:id=""/>
            <category></category>
            <date></date>
            <identification type="volume"></identification>
            <identification type="issue"></identification>
            <tags></tags>
            <share type="facebook"></share>
            <share type="twitter"></share>
            <share type="email"></share>
            <share type="other"></share>
        </header>       
        <body>
            <image url=""></image>
            <p>
                <!-- All of the following tags are optional. -->
                <!-- Location is usually placed at the very begining of the article. -->
                <location></location>
                <!-- Any of the following tags can be used in any order, although a hyperlink will typically follow a website source. -->
                <source type="person"></source>
                <source type="website"></source>
                <Hyperlinks></Hyperlinks>
                <!-- Most articles will use multiple quotes or pieces of information from different sources. -->
                <quote></quote>
            </p>
        </body>
        <footer>
            <share type="facebook"></share>
            <share type="twitter"></share>
            <share type="email"></share>
            <share type="other"></share>
            <related_links url=""></related_links>
        </footer>

            </article>


    <docReview>
        <reviewer type="peer" xml:id="id_here"> Name of Reviewer</reviewer>
        <report type="peer" who="#id_here">
            <p>Peer review report will go here.</p>
        </report>
        <reviewer type="instructor" xml:id="smi.ke">Kevin Smith</reviewer>
        <report type="instructor" who="#smi.ke">
            <p>Instructor comments will go here.</p>
        </report>
    </docReview>
</DOC>
